<mapper namespace="com.wzx.dto.PowerDao">
	<resultMap type="power" id="powermap">
		<id column="id" property="id"/>
		<result column="power_id" property="pname"/>
		<result column="pname" property="pname"/>
	</resultMap>
	
	<select id="getPowerByRole" resultType="power">
		select p.* from midone m,power p where m.p_id=p.id and r_id=#{r_id}
	</select>
</mapper>
